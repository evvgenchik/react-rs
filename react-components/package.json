{
  "name": "react-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server": "node src/serverDate.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "dev": "ts-node server.ts",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build": "npm run build:client && npm run build:server",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .tsx --ext .ts",
    "lint-fix": "eslint . --ext .tsx --ext .ts --fix",
    "server-dev": "json-server -w src/data/booksDB.json -p 3333",
    "format": "prettier --write",
    "cypress:open": "cypress open",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:e2e": "start-server-and-test start http://localhost:5173 cy:open-e2e && npx nyc report --reporter=text-summary",
    "cy:run-unit": "cypress run --e2e"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/aria-query": "^5.0.1",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "json-server": "^0.17.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "sirv": "^2.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.16",
    "@types/json-server": "^0.14.4",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vitest/ui": "^0.29.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.10.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "sass": "^1.59.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0",
    "vite": "^4.2.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3",
    "whatwg-fetch": "^3.6.2"
  }
}
